local library = loadstring(game:HttpGethttps://raw.githubusercontent.com/InRotationX/Aristois-Client/main/thanks'))()

local Window = library:CreateWindow("Aristois-Client", "Version 1.0", 10044538000)

local Tab = Window:CreateTab("Aristois-Client")

local Page = Tab:CreateFrame("Utility")

Button = Page:CreateButton("Staff Detector", "Description", function()
CreateNotification("Staff Detector", "Activated", function(value)
if value == true then
print(true)
else
print(false)
end
end)
end)

Toggle = Page:CreateToggle("Toggle", "test", function(arg)
Toggle:UpdateToggle("Staff Detector", "New Description")
print(arg)
end)

Bind = Page:CreateBind("KeyBind", "F", function(arg)
Bind:UpdateBind("Staff Detector")
print(arg)
end)
